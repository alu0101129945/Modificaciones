
{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": "off",
        "require-jsdoc": "off",
        "prefer-const": "off",
        "eol-last": "off",
        "spaced-comment": "off",
        "no-trailing-spaces": "off",
        "keyword-spacing": "off",
        "valid-jsdoc": "off",
        "space-before-blocks": "off",
        "max-len": "off",
        "indent": "off",
        "array-bracket-spacing": "off",
        "no-unused-vars": "off",
        "new-cap": "off",
        "arrow-parens": "off"
    }
}